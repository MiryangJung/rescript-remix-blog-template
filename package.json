{
  "name": "rescript-remix-blog-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/global.css -o app/styles/global.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/global.css -o app/styles/global.css",
    "res:dev": "rescript build -w"
  },
  "dependencies": {
    "@remix-run/node": "^1.6.1",
    "@remix-run/react": "^1.6.1",
    "@remix-run/vercel": "^1.6.1",
    "@vercel/node": "^1.15.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.1",
    "@remix-run/eslint-config": "^1.6.1",
    "@remix-run/serve": "^1.6.1",
    "@rescript/react": "^0.10.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "eslint": "^8.15.0",
    "postcss": "^8.4.14",
    "rescript": "^9.1.4",
    "rescript-webapi": "^0.6.1",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
